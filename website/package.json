{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "_eslint": "npx eslint -c .eslintrc.cjs --ext .tsx,.ts src",
    "lint:check": "npm run _eslint",
    "lint:fix": "npm run _eslint -- --fix",
    "_prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,html,css,json,yaml,yml}'",
    "format:fix": "npm run _prettier -- --write",
    "format:check": "npm run _prettier -- --check",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "firebase": "^9.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "firebase-tools": "^11.8.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
